{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
      //,"Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5003"
      },
      "Https": {
        "Url": "https://*:5004"
      }
    }
  },
  "Startup": {
    "Cors": {
      "AllowOrigins": "http://localhost:5003,http://127.0.0.1:5003,http://192.168.20.128"
    }
  },
  "tcpLogger": {
    "Address": "http://localhost:15554/",
    "Port": 80,
    "Layout": ""
  },
  "ServerUrl": "https://localhost:7163",
  "ImageServerUrl": "http://localhost:5003",
  "FileServerUrl": "http://localhost:5003",
  "SiteSetting": {
    "WorkerId": 1, //for snowflake workerid
    "DataCenterId": 1, //for snowflake datacenterid
    "LoginFailedCountLimits": 3, //the number of login failed 
    "LoginLockedTimeout": 3 //(minutes) account locked timeout
  },
  "Redis": {
    "Useable": false,
    "EnableBloomFilter": true,
    "DefaultKey": "redis_test",
    "Dbconfig": {
      "ConnectionString": "192.168.20.128:6381,ssl=false,sslHost=null,connectTimeout=4000,allowAdmin=true"
    }
  },
  "Rabbitmq": {
    "HostName": "192.168.20.128",
    "VirtualHost": "/",
    "UserName": "admin",
    "Password": "football",
    "Port": 13572
  },
  "site": "cms.Core.Admin.WebApi",
  "mode": "dev",
  "RSAmode": "old",
  "DataConfig": {
    "Sqlserver": {
      "ConnectionString": "Data Source=.;Initial Catalog=cms_core_copy;uid=sa;pwd=123456;TrustServerCertificate=true;MultipleActiveResultSets=True;",
      "Enable": "true"
    },
    "Mysql": {
      "ConnectionString": "",
      "Enable": "false"
    }
  },
  "JwtSetting": {
    "Issuer": "Flex.cms.Web",
    "Audience": "Flex.cms.Audience",
    "SecurityKey": "Flex.cms.WebHost_SecurityKeyYouCanFreeForSetIt", //more than 32 chars
    "LifeTime": 1440, //(minutes) token life time default:1440 m=1 day
    "RefreshTokenExpire": 10080 //(minutes) token life time default:10080 m=7 day
  }
}
