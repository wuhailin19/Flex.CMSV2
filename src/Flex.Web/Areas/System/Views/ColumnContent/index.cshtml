<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link href="~/Scripts/layui/css/layui.css" rel="stylesheet" />
    <script src="~/Scripts/layui/layui.js" type="text/javascript"></script>
    <script src="~/scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/route.js" type="text/javascript"></script>
    <link href="~/system/css/columncontent/content.css" rel="stylesheet" />
    <script src="~/system/js/columncontent/openpageoptions.js"></script>
    <link href="~/system/css/columncontent/font/iconfont.css" rel="stylesheet" />
</head>
<body>
    <form action="" onclick="return false;" class="search-box">
        <div class="layui-form-item">
            <div class="layui-inline">
                <label class="layui-form-label">搜索</label>
                <div class="layui-input-inline listsearch_box" style="display:flex;">
                    <input type="text" name="keyword" id="keyword" lay-verify="keyword" placeholder="关键字或ID" class="layui-input">
                    <button class="layui-btn" data-type="reload">搜索</button>
                </div>
                @* <div class="layui-inline search_box">

                </div> *@
            </div>
            <div class="layui-inline">
                <label class="layui-form-label">日期范围</label>
                <div class="layui-inline" id="ID-laydate-range">
                    <div class="layui-input-inline" style="display:flex;">
                        <input type="text" autocomplete="off" id="ID-laydate-start-date" class="layui-input start-date" placeholder="开始日期">
                        <div class="layui-form-mid">-</div>
                        <input type="text" autocomplete="off" id="ID-laydate-end-date" class="layui-input end-date" placeholder="结束日期">
                    </div>
                </div>
            </div>
        </div>
    </form>
    <div class="layui-tab-content">
        <table id="demo_tree" lay-filter="test"></table>
    </div>
    <script type="text/html" id="statusPxy">
        <span name="StatusCode" style="color:{{d.StatusColor}}">{{d.StatusCode}}</span>
    </script>
    <script type="text/html" id="Property">
        <div class="Propertybox">
        {{#if(d.IsTop){}}
        <span class="Property top iconfont">&#xe60e;</span>
        {{#}else{}}

        {{#}}}
        {{#if(d.IsRecommend){}}
        <span class="Property recommend iconfont">&#xe63b;</span>
        {{#}else{}}

        {{#}}}
        {{#if(d.IsHot){}}
        <span class="Property hot iconfont">&#xe8c9;</span>
        {{#}else{}}

        {{#}}}
        {{#if(d.IsSilde){}}
        <span class="Property focus iconfont">&#xe686;</span>
        {{#}else{}}

        {{#}}}
        {{#if(d.IsHide){}}
        <span class="Property hide iconfont">&#xe6d1;</span>
        {{#}else{}}

        {{#}}}
        </div>
    </script>
    @* <script type="text/html" id="toolbarDemo">
    <div class="layui-btn-container">
    <button class="layui-btn layui-btn-sm" lay-event="addRole">添加</button>
    <button class="layui-btn layui-btn-sm" lay-event="Edit">编辑</button>
    <button class="layui-btn layui-btn-sm layui-btn-danger" lay-event="deleteAll">删除</button>
    </div>
    </script> *@
    <script src="~/system/js/columncontent/index.js"></script>
</body>
</html>