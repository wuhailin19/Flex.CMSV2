@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link href="~/Scripts/layui/css/layui.css" rel="stylesheet" />
    <!--link rel="stylesheet" href="../style/laypro/layuiadmin/style/admin.css" media="all"-->
    <link href="~/Scripts/core/laypro/layui-ext.css" rel="stylesheet" />
    <link href="~/Scripts/core/myflow-min/index.css" rel="stylesheet" />
    <link href="~/Scripts/core/myflow-min/lib/jquery-ui-1.8.4.custom/css/smoothness/jquery-ui-1.8.4.custom.css" rel="stylesheet" />
    <script src="~/Scripts/core/myflow-min/lib/raphael-min.js"></script>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/core/myflow-min/lib/jquery-ui-1.8.4.custom/js/jquery-ui-1.8.4.custom.min.js"></script>
    <script src="~/Scripts/core/myflow-min/lib/jquery-ui-1.8.4.custom/js/jquery-ui-1.8.4.custom.min.js"></script>
    <script src="~/Scripts/core/myflow-min/myflow.js"></script>
    <script src="~/Scripts/core/myflow-min/myflow.jpdl4.js"></script>
    <script src="~/Scripts/core/myflow-min/myflow.editors.js"></script>
    <script src="~/Scripts/core/commonUtil.js"></script>
    <script src="~/Scripts/layer/layer.js"></script>
    <script src="~/scripts/layui/layui.js"></script>
    <style>
        .lay-selet { width: 100%; padding-left: 10px; height: 38px; border-width: 1px; border-style: solid; border-color: #e6e6e6; border-radius: 2px; }
    </style>
    <script src="~/Scripts/route.js"></script>
    <script src="~/system/js/workflow/designworkflow.js"></script>
</head>
<body onload="javascript:init();">

    <div id="myflow_tools"
         style="position: absolute; top: 30px; left: 30px; background-color: #fff; width: 80px; cursor: pointer; padding: 4px;"
         class="ui-widget-content">
        <div id="myflow_tools_handle" style="text-align: center;cursor: move;" class="layui-card-header">工具集</div>
        <div class="node" id="myflow_save">
            <img src="~/Content/myflow-min/img/disk.png" />&nbsp;&nbsp;保存
        </div>
        <!--div class="node" id="myflow_revoke"><img src="~/Scripts/core/myflow-min/save.gif" />&nbsp;&nbsp;撤销</div-->
        <div>
            <hr />
        </div>
        <div class="node selectable" id="pointer">
            <img src="~/Content/myflow-min/img/select16.gif" />&nbsp;&nbsp;选择
        </div>
        <div class="node selectable" id="path">
            <img src="~/Content/myflow-min/img/16/flow_sequence.png" />&nbsp;&nbsp;动作
        </div>
        <div>
            <hr />
        </div>
        <div class="node state" id="start" type="start">
            <img src="~/Content/myflow-min/img/16/start_event_empty.png" />&nbsp;&nbsp;开始
        </div>
        <!--div class="node state" id="state" type="state"><img
            src="~/Content/myflow-min/img/16/task_empty.png" />&nbsp;&nbsp;状态</div-->
        <div class="node state" id="task" type="task">
            <img src="~/Content/myflow-min/img/16/task_empty.png" />&nbsp;&nbsp;步骤
        </div>
        <!--div class="node state" id="fork" type="fork"><img
            src="~/Content/myflow-min/img/16/gateway_parallel.png" />&nbsp;&nbsp;分支</div>
        <div class="node state" id="join" type="join"><img
            src="~/Content/myflow-min/img/16/gateway_parallel.png" />&nbsp;&nbsp;合并</div-->
        <div class="node state" id="end" type="end">
            <img src="~/Content/myflow-min/img/16/end_event_terminate.png" />&nbsp;&nbsp;结束
        </div>
        <div class="node state" id="end-error" type="end-error">
            <img src="~/Content/myflow-min/img/16/end_event_error.png" />&nbsp;&nbsp;取消
        </div>
        <div class="node state" id="end-cancel" type="end-cancel">
            <img src="~/Content/myflow-min/img/16/end_event_cancel.png" />&nbsp;&nbsp;否决
        </div>

    </div>

    <div id="myflow_props"
         style="position: absolute; top: 30px; right: 50px; background-color: #fff; width: 220px; padding: 3px;"
         class="ui-widget-content">
        <div id="myflow_props_handle" class="layui-card-header" style="cursor: move;">属性</div>
        <table class="layui-table" lay-skin="line" width="100%" cellpadding="0" cellspacing="0">
            <tr>
                <td></td>
            </tr>
            <tr>
                <td></td>
            </tr>
        </table>
        <div>&nbsp;</div>
    </div>

    <div id="myflow"></div>
</body>
</html>
