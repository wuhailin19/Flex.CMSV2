<!DOCTYPE html>
<html lang="en" xmlns:th=http://www.thymeleaf.org>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=0,maximum-scale=0,user-scalable=yes,shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>表单设计器</title>
    <link href="~/scripts/layui/css/layui.css" rel="stylesheet" />
    <link href="~/scripts/layui/module/formdesigner/labelgeneration.css" rel="stylesheet" />
    <link href="~/scripts/layui/module/formdesigner/formdesigner.css" rel="stylesheet" />
    <script src="~/scripts/jquery-3.4.1.min.js"></script>
    @* <script src="~/system/htmleditor/_ueditor/editor_config.js"></script>
    <script src="~/system/htmleditor/_ueditor/editor_all.js"></script> *@
    <script src="~/system/htmleditor/_ueditor/ueditor.config.js"></script>
    <script src="~/system/htmleditor/_ueditor/ueditor.all.js"></script>
    <script src="~/system/htmleditor/_ueditor/lang/zh-cn/zh-cn.js"></script>
    <script src="~/system/htmleditor/_ueditor/ckeditor.min.office.js"></script>
    <link href="~/system/css/uploader/webuploadercustom.css" rel="stylesheet" />
    <script src="~/system/js/uploader/webuploader.min.js"></script>
    <script src="~/system/js/uploader/uploadsingle.js"></script>
</head>

<body class="layui-layout-body">
    <!--<button type="button" id="test">aa</button>-->
    <input type="hidden" th:value="${formId}" id="fromId">
    <div style="height: 100%; width: 100%;" id="formdesigner">
    </div>
    <script src="~/scripts/layui/layui.js"></script>
    <script src="~/scripts/route.js"></script>
    <script src="~/scripts/layui/module/formdesigner/sortable.js"></script>
    <!--htmlformat 为js-beautify代码-->
    <script src="~/scripts/layui/module/formdesigner/htmlformat.js"></script>
    <script src="~/scripts/layui/module/formdesigner/formfield.js"></script>
    <script src="~/scripts/layui/module/formdesigner/jsformat.js"></script>

    <script>
        layui.config(
            { base: '/Scripts/layui/module/FormDesigner/' }).use(['layer', 'formDesigner'], function () {
                var formDesigner = layui.formDesigner;
                var data = [];
                var render = formDesigner.render({
                    data: data,
                    elem: '#formdesigner'
                });

            });
    </script>

</body>

</html>